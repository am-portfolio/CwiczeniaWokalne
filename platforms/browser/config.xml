<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.hellocordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Ćwiczenia Wokalne</name>
    <description>
        Dopiero się uczysz, a może śpiewasz zawodowo? Dzięki tej aplikacji dopasuj ćwiczenia wokalne pod własne potrzeby. Ty decydujesz co i jak chcesz ćwiczyć.
    </description>
    <author email="aron.mandrella@gmail.com" href="http://">
        Aron Mandrella
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
</widget>
